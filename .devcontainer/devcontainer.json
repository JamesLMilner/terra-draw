{
	"name": "terra-draw",
	"dockerFile": "Dockerfile",
	"build": {
		"args": {
			"NODE_MAJOR_VERSION": "20"
		}
	},
	"postCreateCommand": [".devcontainer/post-create.sh"],
	"portsAttributes": {
		"3000": {
			"label": "Docs"
		},
		"6006": {
			"label": "Storybook"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"esbenp.prettier-vscode",
				"mikestead.dotenv"
			]
		}
	}
}
