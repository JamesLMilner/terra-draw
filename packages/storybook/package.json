{
	"name": "terra-draw-storybook",
	"version": "0.0.1",
	"private": true,
	"description": "Storybook examples for Terra Draw components and features",
	"main": "src/index.ts",
	"scripts": {
		"storybook": "storybook dev -p 6006",
		"storybook:build": "storybook build",
		"storybook:build:memory": "NODE_OPTIONS=--max-old-space-size=4096 storybook build",
		"storybook:serve": "http-server storybook-static --port 6006 --silent",
		"storybook:test": "test-storybook --ci --disable-telemetry --failOnConsole --excludeTags exclude",
		"storybook:help": "test-storybook --help"
	},
	"author": "James Milner",
	"license": "MIT",
	"dependencies": {
		"@arcgis/core": "4.31.6",
		"@googlemaps/js-api-loader": "1.14.3",
		"@storybook/mdx2-csf": "^1.1.0",
		"@types/leaflet": "1.9.15",
		"leaflet": "1.9.4",
		"mapbox-gl": "3.9.2",
		"maplibre-gl": "5.6.1",
		"ol": "10.3.1"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "8.6.14",
		"@storybook/addon-interactions": "^8.6.14",
		"@storybook/addon-links": "8.6.14",
		"@storybook/html": "8.6.14",
		"@storybook/html-vite": "8.6.14",
		"@storybook/test": "^8.6.14",
		"@storybook/test-runner": "^0.23.0",
		"http-server": "^14.1.1",
		"storybook": "8.6.14",
		"typescript": "5.6.3"
	}
}
